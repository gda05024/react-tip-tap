#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mildang/react-tip-tap/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.20.3_@types+node@22.14.0_@types+react@19.1.0_postcss_ekbqnyopo6nulmp6xyd6c7s4fq/node_modules/vitepress/bin/node_modules:/Users/mildang/react-tip-tap/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.20.3_@types+node@22.14.0_@types+react@19.1.0_postcss_ekbqnyopo6nulmp6xyd6c7s4fq/node_modules/vitepress/node_modules:/Users/mildang/react-tip-tap/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.20.3_@types+node@22.14.0_@types+react@19.1.0_postcss_ekbqnyopo6nulmp6xyd6c7s4fq/node_modules:/Users/mildang/react-tip-tap/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mildang/react-tip-tap/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.20.3_@types+node@22.14.0_@types+react@19.1.0_postcss_ekbqnyopo6nulmp6xyd6c7s4fq/node_modules/vitepress/bin/node_modules:/Users/mildang/react-tip-tap/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.20.3_@types+node@22.14.0_@types+react@19.1.0_postcss_ekbqnyopo6nulmp6xyd6c7s4fq/node_modules/vitepress/node_modules:/Users/mildang/react-tip-tap/node_modules/.pnpm/vitepress@1.6.3_@algolia+client-search@5.20.3_@types+node@22.14.0_@types+react@19.1.0_postcss_ekbqnyopo6nulmp6xyd6c7s4fq/node_modules:/Users/mildang/react-tip-tap/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../vitepress/bin/vitepress.js" "$@"
fi
