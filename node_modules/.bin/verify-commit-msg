#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mildang/react-tip-tap/node_modules/.pnpm/verify-commit-msg@0.0.14/node_modules/verify-commit-msg/bin/node_modules:/Users/mildang/react-tip-tap/node_modules/.pnpm/verify-commit-msg@0.0.14/node_modules/verify-commit-msg/node_modules:/Users/mildang/react-tip-tap/node_modules/.pnpm/verify-commit-msg@0.0.14/node_modules:/Users/mildang/react-tip-tap/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mildang/react-tip-tap/node_modules/.pnpm/verify-commit-msg@0.0.14/node_modules/verify-commit-msg/bin/node_modules:/Users/mildang/react-tip-tap/node_modules/.pnpm/verify-commit-msg@0.0.14/node_modules/verify-commit-msg/node_modules:/Users/mildang/react-tip-tap/node_modules/.pnpm/verify-commit-msg@0.0.14/node_modules:/Users/mildang/react-tip-tap/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../verify-commit-msg/bin/cli.mjs" "$@"
else
  exec node  "$basedir/../verify-commit-msg/bin/cli.mjs" "$@"
fi
